   "source": [
    "# Problem 1-2. Implement Logistic Regression Model\n",
    "\n",
    "### sklearn 없이 Logistic Regression 구현하기\n",
    "\n",
    "-- Implement Logistic Regression without using scikit-learn libraries.\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 21,
   "metadata": {
    "id": "eGTRLxvsGbS7"
   },
   "outputs": [],
   "source": [
    "def sigmoid(z):\n",
    "    # YOUR CODE COMES HERE\n",
    "     return 1/ (1 + np.exp(-z))\n",
    "\n",
    "def binary_cross_entropy_loss(y_pred, target):\n",
    "    # YOUR CODE COMES HERE\n",
    "    loss = (-target * np.log(y_pred + 1e-9) - (1-target)*np.log(1-y_pred + 1e-9))\n",
    "    return loss.mean()\n",
    "\n",
    "def learn_and_return_weights_numpy(X, Y, lr=.01, iter=400000):\n",
    "    # YOUR CODE COMES HERE\n",
    "    num_ex, num_features = np.shape(X)\n",
    "    intercept = np.ones((num_ex, 1))\n",
    "    X = np.concatenate((intercept, X), axis=1)\n",
    "    W = np.zeros(num_features + 1)\n",
    "    \n",
    "    loss = []\n",
    "    for i in range(iter): #반복\n",
    "        z = np.dot(X, W)\n",
    "        h = sigmoid(z) #sigmoid 함수 사용\n",
    "        loss += [binary_cross_entropy_loss(h, Y)]\n",
    "        \n",
    "        grad = np.dot(X.transpose(), h-Y) / num_ex\n",
    "        \n",
    "        W -= lr * grad\n",
    "        \n",
    "    # w: coefficient of the model to input features,\n",
    "    # b: bias of the model\n",
    "    w = W[1:num_features + 1]\n",
    "    b = W[0] \n",
    "    \n",
    "    return w, b"
   ]
